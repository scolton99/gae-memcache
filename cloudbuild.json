{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/go",
      "args": ["install"],
      "env": ["GOPATH=/workspace/go"]
    },
    {
      "name": "gcr.io/cloud-builders/go",
      "args": ["build"],
      "env":  ["GOPATH=/workspace/go"]
    },
    {
      "name": "gcr.io/cloud-builders/gcloud",
      "args": ["app", "deploy"],
      "env":  ["GOPATH=/workspace/go"]
    }
  ]
}